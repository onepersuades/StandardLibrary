<div class="wrap">
	<h2><i class="dashicons dashicons-admin-generic"></i>&nbsp;{{ title }}</h2>
	{{ wp_nonce_field.meta_box }}
	{{ wp_nonce_field.postboxes }}
	<div id="poststuff">
		<div id="post-body" class="metabox-holder columns-{{ screen_cols }}"> 
			<div id="postbox-container-1" class="postbox-container">
				{{ do_meta_boxes( "side" ) }}
			</div>    
			<div id="postbox-container-2" class="postbox-container">
				{{ do_meta_boxes( "normal" ) }}
				{{ do_meta_boxes( "advanced" ) }}
			</div>	     					
		</div>
	</div>
</div>
<script type="text/javascript">
postboxes.add_postbox_toggles( pagenow );
</script>